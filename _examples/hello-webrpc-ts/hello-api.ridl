webrpc = v1

name = hello-webrpc
version = v1.0.0

enum Kind: uint32
  - USER = 1
  - ADMIN = 2

message Empty

message GetUserRequest
  - userID: uint64

message User
  - ID: uint64
    + json = id
    + go.tag.db = id

  - username: string
    + json = USERNAME
    + go.tag.db = username

  - createdAt?: timestamp
    + json = created_at,omitempty
    + go.tag.db = created_at


service ExampleService
  - Ping(): bool
  - GetUser(GetUserRequest): User

